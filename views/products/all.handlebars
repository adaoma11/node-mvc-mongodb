<h1>Produtos</h1>

<div class="products">

    {{#each products}}
        <div class="product">
            <h3>{{this.name}}</h3>
            <img src="{{this.image}}" alt="{{this.name}}">
            <p>R$ {{this.price}}</p>
            <p>Descrição: {{this.description}}</p>
        </div>
    {{/each}}

</div>